{
    "compilerOptions": {
        "target": "es2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "rootDir": "./src",
        "sourceMap": true,
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "ignoreDeprecations": "6.0"
    }
}